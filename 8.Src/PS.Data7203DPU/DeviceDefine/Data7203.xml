<?xml version="1.0" encoding="utf-8" ?>
<devicedefines>
  <devicedefine devicetype="Data7203" devicetext="Data7203">
    <operadefine name="read">
      <sendpart>
        <datafield name="address"
            begin="0"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>
        
        <datafield name="df2"
            begin="1"
            length="5"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="04 13 89 00 04"/>

        <datafield name="crc"
             begin="6"
             length="2"
             converter="Xdgk.Communi.OriginalConverter"
             crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="13">
        <datafield name="address"
            begin="0"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df1"
            begin="1"
            length="2"
            bytes="04 08"
            converter="Xdgk.Communi.OriginalConverter"/>
        
        <datafield name="sum"
            begin="3"
            length="4"
            converter="Xdgk.Communi.Int32ModbusConverter" />

        <datafield name="if"
            begin="7"
            length="4">
            <converter name="Xdgk.Communi.DecimalValueAdapterConverter" hasInner="true">
              <property name="Rate" value="100"></property>
              <converter name="Xdgk.Communi.Int32ModbusConverter"></converter>
            </converter>
        </datafield>

        <datafield name="crc"
            begin="11"
            length="2"
             converter="Xdgk.Communi.OriginalConverter"
             crc="true"/>

        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>
  </devicedefine>
</devicedefines>
