            bytes="03" />

        <!-- 263 ~ 338 -->
        <datafield name="datas"
            begin="2"
            length="4"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="01 05 00 4C" />

        <datafield name="crc"
            begin="6"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true" />

        <crcer name="Xdgk.Communi.CRC16" />
      </sendpart>

      <receivepart length="157">
