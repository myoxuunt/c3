        <datafield name="OTSet" begin="151" length="4" converter="Xdgk.Communi.FloatModbusConverter"/>
        
        <datafield name="crc"
            begin="155"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true" />

        <crcer name="Xdgk.Communi.CRC16" />
      </receivepart>
    </operadefine>

