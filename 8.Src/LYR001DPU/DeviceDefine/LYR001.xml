    <operadefine name="WriteOT">
      <sendpart>
        <datafield name="address"
            begin="0"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df1"
            begin="1"
            length="6"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="10 01 4F 00 02 04"/>

        <datafield name="OT"
            begin="7"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"
            lazy="true"/>

        <datafield name="crc"
            begin="11"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true" />
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="8">
        <datafield name="address"
            begin="0"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="fc"
            begin="1"
            length="1"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="10"
            matchcheck="true" />

        <datafield name="beginPosition"
            begin="2"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="01 4F" />

        <datafield name="datalength"
            begin="4"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="00 02" />

        <datafield name="crc"
            begin="6"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16" />
      </receivepart>
    </operadefine>

    <operadefine name="WriteOTMode">
      <sendpart>
        <datafield name="address"
            begin="0"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df1"
            begin="1"
            length="6"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="0F 00 18 00 01 01"/>

        <datafield name="Mode"
            begin="7"
            length="1"
            converter="Xdgk.Communi.Int8Converter"
            lazy="true"/>

        <datafield name="crc"
            begin="8"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true" />
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="8">
        <datafield name="address"
            begin="0"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df1"
            begin="1"
            length="5"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="0F 00 18 00 01"
            matchcheck="true" />

        <datafield name="crc"
            begin="6"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>

        <crcer name="Xdgk.Communi.CRC16" />
      </receivepart>
    </operadefine>


    <operadefine name="ReadM" isComplex="true">
      <opera name="ReadReal"/>
      <opera name="ReadStatus"/>
    </operadefine>

