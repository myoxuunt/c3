<?xml version="1.0" encoding="utf-8" ?>
<devicedefines>
  <devicedefine devicetype="LYR001Device" devicetext="LYR001">
    <operadefine name="ReadReal" processfd="true">
      <sendpart>
        <datafield name="address"
            begin="0"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df1"
            begin="1"
            length="5"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="03 01 53 00 1E"/>

        <datafield name="crc"
            begin="6"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true" />
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="65">
        <datafield name="address"
            begin="0"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="fc"
            begin="1"
            length="1"
            converter="Xdgk.Communi.Int8Converter"
            bytes="03"
            matchcheck="true"/>

        <datafield name="datalen"
            begin="2"
            length="1"
            converter="Xdgk.Communi.Int8Converter"
            bytes="3C"
            matchcheck="true"/>

        <datafield name="OD"
            begin="3"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="OT"
            begin="7"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="GT1"
            begin="11"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="BT1"
            begin="15"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="GT2"
            begin="19"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="BT2"
            begin="23"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="GP1"
            begin="27"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="BP1"
            begin="31"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="GP2"
            begin="35"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="BP2"
            begin="39"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="WL"
            begin="43"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="IR"
            begin="47"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="SR"
            begin="51"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="I1"
            begin="55"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="S1"
            begin="59"
            length="4"
            converter="Xdgk.Communi.FloatModbusConverter"/>

        <datafield name="crc"
            begin="63"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>

        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>


    <!--read modbus control line-->
    <operadefine name="ReadAdjustControl">
      <sendpart>
        <datafield name="address"
