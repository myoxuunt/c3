        <datafield name="fc"
            begin="1"
            length="1"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="02" />

        <datafield name="datas"
            begin="2"
            length="4"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="00 50 00 11" />

        <datafield name="crc"
            begin="6"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true" />

        <crcer name="Xdgk.Communi.CRC16" />
      </sendpart>

      <receivepart length="8">
        <datafield name="address"
