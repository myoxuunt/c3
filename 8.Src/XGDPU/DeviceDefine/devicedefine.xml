<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE devicedefines []>
<devicedefines>
  <!--巡更控制器-->
  <devicedefine devicetype="xgdevice" devicetext="巡更控制器">
    <!--读取记录条数-->
    <operadefine name="readcount" text ="读取巡更记录总数">
      <sendpart>
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"/>

        <datafield name="address"
            begin="3"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 06 00"/>

        <datafield name="crc"
            begin="7"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="10">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true"/>

        <datafield name="df2"
            begin="4"
            length="3"
            bytes="B0 06 01"
            converter="Xdgk.Communi.OriginalConverter"
            matchcheck="true"/>

        <datafield name="recordcount"
            begin="7"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="crc"
            begin="8"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>

        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>

    <!--读取控制器日期-->
    <operadefine name="readdate" text="读取控制器日期">
      <sendpart>
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"/>

        <datafield name="address"
            begin="3"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 04 00"/>

        <datafield name="crc"
            begin="7"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="12">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true"/>

        <datafield name="df2"
            begin="4"
            length="3"
            bytes="B0 04 03"
            converter="Xdgk.Communi.OriginalConverter"
            matchcheck="true"/>

        <datafield name="date"
            begin="7"
            length="3"
            converter="Xdgk.Communi.XGDateConverter"/>

        <datafield name="crc"
            begin="10"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>

        <crcer name="Xdgk.Communi.CRC16"/>

      </receivepart>
    </operadefine>

    <!--读取控制器时间-->
    <operadefine name="readtime" text="读取控制器时间">
      <sendpart>
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"/>

        <datafield name="address"
            begin="3"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 03 00"/>

        <datafield name="crc"
            begin="7"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="12">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true"/>

        <datafield name="df2"
            begin="4"
            length="3"
            bytes="B0 03 03"
            converter="Xdgk.Communi.OriginalConverter"
            matchcheck="true"/>

        <datafield name="time"
            begin="7"
            length="3"
            converter="Xdgk.Communi.XGTimeConverter"/>

        <datafield name="crc"
            begin="10"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>

        <crcer name="Xdgk.Communi.CRC16"/>

      </receivepart>
    </operadefine>

    <!--写入控制器日期 -->
    <operadefine name="writedate" text="写入控制器日期">
      <sendpart>
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true"/>

        <datafield name="address"
            begin="3"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df2"
            begin="4"
            length="3"
            bytes="B0 02 03"
            converter="Xdgk.Communi.OriginalConverter"
            matchcheck="true"/>

        <datafield name="date"
            begin="7"
            length="3"
            lazy="true"
            converter="Xdgk.Communi.XGDateConverter"/>

        <datafield name="crc"
            begin="10"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>

        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="9">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"/>

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 02 00"/>

        <datafield name="crc"
            begin="7"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>

    <!--写入控制器时间 -->
    <operadefine name="writetime" text="写入控制器时间">
      <sendpart>
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"/>

        <datafield name="address"
            begin="3"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 01 03"/>

        <datafield name="time"
            begin="7"
            length="3"
            lazy="true"
            converter="Xdgk.Communi.XGTimeConverter"/>

        <datafield name="crc"
            begin="10"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="9">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true"/>

        <datafield name="df2"
            begin="4"
            length="3"
            bytes="B0 01 00"
            converter="Xdgk.Communi.OriginalConverter"
            matchcheck="true"/>

        <datafield name="crc"
            begin="7"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>

    <!--自动上报-->
    <operadefine name="recordupload" processfd="true">
      <sendpart>
      </sendpart>

      <receivepart length="25">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true" />

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 0A 10"
            matchcheck="true"/>

        <datafield name="DT"
            begin="7"
            length="8"
            converter="Xdgk.Communi.XGDateTimeConverter"/>

        <datafield name="cardsn"
            begin="15"
            length="8"
            converter="Xdgk.Communi.CardSNConverter" />

        <datafield name="crc"
            begin="23"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>

    <!--读取巡更数据-->
    <operadefine name="readrecord" processfd="true"
        text="读取巡更记录" args="recordidx">
      <sendpart>
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"/>

        <datafield name="address"
            begin="3"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 07 01"/>

        <datafield name="recordidx"
            begin="7"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="crc"
            begin="8"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart name="record" length="25">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true" />

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 07 10"
            matchcheck="true"/>

        <datafield name="DT"
            begin="7"
            length="8"
            converter="Xdgk.Communi.XGDateTimeConverter"/>

        <datafield name="cardsn"
            begin="15"
            length="8"
            converter="Xdgk.Communi.CardSNConverter" />

        <datafield name="crc"
            begin="23"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>

    <!--清除巡更记录-->
    <operadefine name="clearrecord" text="清除巡更记录">
      <sendpart>
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"/>
        <datafield name="address"
            begin="3"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>
        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 08 00"/>
        <datafield name="crc"
            begin="7"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>

        <crcer name="Xdgk.Communi.CRC16"/>

      </sendpart>

      <receivepart length="9">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true" />

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 08 00"
            matchcheck="true"/>

        <datafield name="crc"
            begin="7"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>

    <!--删除本次上传的巡更记录-->
    <operadefine name="removeupload" text="删除上传记录">
      <sendpart>
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"/>
        <datafield name="address"
            begin="3"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>
        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 0A 00"/>
        <datafield name="crc"
            begin="7"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>

        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>

      <receivepart length="0"/>
    </operadefine>

    <operadefine name="WriteMode">
      <sendpart>
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"/>

        <datafield name="address"
            begin="3"
            length="1"
            converter="Xdgk.Communi.Int8Converter"/>

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 0B 01"/>

        <datafield name="mode"
            begin="7"
            length="1"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="02"/>

        <datafield name="crc"
            begin="8"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </sendpart>
      
      <receivepart length="9">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true" />

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 0B 00"
            matchcheck="true"/>

        <datafield name="crc"
            begin="7"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </operadefine>

    <uploaddefine name="recordupload">
      <receivepart length="25">
        <datafield name="df1"
            begin="0"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="21 58 44"
            matchcheck="true" />

        <datafield name="df2"
            begin="4"
            length="3"
            converter="Xdgk.Communi.OriginalConverter"
            bytes="B0 0A 10"
            matchcheck="true"/>

        <datafield name="DT"
            begin="7"
            length="8"
            converter="Xdgk.Communi.XGDateTimeConverter"/>

        <datafield name="cardsn"
            begin="15"
            length="8"
            converter="Xdgk.Communi.CardSNConverter" />

        <datafield name="crc"
            begin="23"
            length="2"
            converter="Xdgk.Communi.OriginalConverter"
            crc="true"/>
        <crcer name="Xdgk.Communi.CRC16"/>
      </receivepart>
    </uploaddefine>
  </devicedefine>
</devicedefines>
